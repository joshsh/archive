        <owl:disjointWith rdf:resource="#SPARQLQuery"/>
        <owl:disjointWith rdf:resource="#WebResourceItem"/>
        <rdfs:comment
            >A first-class item corresponding to a file.</rdfs:comment>
    </owl:Class>
    


