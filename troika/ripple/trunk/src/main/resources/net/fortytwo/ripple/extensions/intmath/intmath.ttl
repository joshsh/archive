@prefix rpl:       <http://fortytwo.net/2007/03/ripple#> .
@prefix intmath:   <http://fortytwo.net/2007/03/ripple/intmath#> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#> .

intmath:abs
	rdfs:label "integer-abs" ;
	a rpl:PrimitiveFunction ;
	rpl:arity 1 ;
	rpl:returnType xsd:integer ;
	rpl:parameters (
		[
			rdfs:label "x" ;
			rpl:parameterType xsd:integer ;
			rpl:parameterTransparency true ]
	) .

