@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rpl:       <http://fortytwo.net/2007/03/ripple/schema#> .
@prefix rpl-test:  <http://fortytwo.net/2007/03/rpl-test#> .
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#> .

### RDF primitives #############################################################

rpl-test:bagElements
	a rpl:Function ;
	rpl:arity 1 ;
	rpl:returnType rdf:Resource ;
	rpl:parameters (
		[
			rdfs:label "bag" ;
			rpl:parameterType rdf:Bag ;
			rpl:isTransparent true ]
		) ;
	rdfs:label "" ;
	rdfs:comment "..." .

rpl-test:listElements
	a rpl:Function ;
	rpl:arity 1 ;
	rpl:returnType rdf:Resource ;
	rpl:parameters (
		[ rdfs:label " listHead" ;
		rpl:parameterType rdf:List ;
		rpl:isTransparent true ]
		) ;
	rdfs:label "" ;
	rdfs:comment "..." .

### String manipulation primitives #############################################

rpl-test:concatenateStrings
	a rpl:Function ;
	rpl:arity 2 ;
	rpl:returnType xsd:string ;
	rpl:parameters (
		[
			rdfs:label "strA" ;
			rpl:parameterType xsd:string ;
			rpl:isTransparent true ]
		[
			rdfs:label "strB" ;
			rpl:parameterType xsd:string ;
			rpl:isTransparent true ]
		) ;
	rdfs:label "" ;
	rdfs:comment "..." .
