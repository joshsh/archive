@prefix rpl:       <http://fortytwo.net/2007/03/rpl#> .
@prefix rpl-test:  <http://fortytwo.net/2007/03/rpl-test#> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#> .

### Integer arithmetic primitives ##############################################

rpl-test:integer-add
	rdfs:type rpl:PrimitiveFunction ;
	rpl:returnType xsd:integer ;
	rpl:parameters (
		[
			rdfs:label "intA" ;
			rpl:parameterType xsd:integer ;
			rpl:parameterTransparency true ]
		[
			rdfs:label "intB" ;
			rpl:parameterType xsd:integer ;
			rpl:parameterTransparency true ]
	) .

