### RDF primitives #############################################################

rpl-test:bagElements
	rdfs:type rpl:PrimitiveFunction ;
	rpl:returnType rdf:Resource ;
	rpl:parameters (
		[
			rdfs:label "bag" ;
			rpl:parameterType rdf:Bag ;
			rpl:parameterTransparency true ]
	) .

rpl-test:listElements
	rdfs:type rpl:PrimitiveFunction ;
	rpl:returnType rdf:Resource ;
	rpl:parameters (
		[ rdfs:label " listHead" ;
		rpl:parameterType rdf:List ;
		rpl:parameterTransparency true ]
	) .

