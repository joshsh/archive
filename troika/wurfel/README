rapper -q -i turtle -o rdfxml ./src/main/resources/wurfel/schema/wurfel.rdf.turtle.txt > ./src/main/resources/wurfel/schema/wurfel.rdf
rapper -q -i turtle -o rdfxml ./src/main/resources/wurfel/schema/wurfel-test.rdf.turtle.txt > ./src/main/resources/wurfel/schema/wurfel-test.rdf

$RAPTORTRUNK/utils/rapper -q -i rdfxml -o dot ./src/main/resources/wurfel/schema/wurfel-test.rdf > ./src/main/resources/wurfel/schema/wurfel-test.dot
dot -Tpng -o ./src/main/resources/wurfel/schema/wurfel-test.png ./src/main/resources/wurfel/schema/wurfel-test.dot

